
<p id="notice"><%= notice %></p>

<div class="jumbotron">
<div class="panel">
  <% if @user.first_name %>
    <h1>Welcome to your dashboard, <%= @user.first_name%></h1>
    <% if @listings %>
      <% @listings.each do |listing| %>
        <%= render 'listings/dashlisting', :locals => {
            :listing => listing
          } %>
      <% end %>
    <% end %>
  <% else %>
    <h1>Create your profile</h1>
    <!-- Render the profile creation panel etc -->
    <%=
    #  render 'hunter_profiles/form'
    %>
  <% end %>
</div>


<div class="text-center">
<%= link_to "Sign Out", destroy_user_session_path, :method => :delete, class: 'btn btn-primary btn-lg' %>
</div>
</div>
