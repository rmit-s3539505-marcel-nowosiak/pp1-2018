<div class="listing">
<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @listings.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @listings.body %>
</p>

<p>
  <strong>Min Salary:</strong>
  <%= @listings.min_salary %>
</p>

<p>
  <strong>Max Salary:</strong>
  <%= @listings.max_salary %>
</p>

<p>
  <strong>Location:</strong>
  <%= @listings.location %>
</p>
</div>
<% if @is_employer %>
<%= link_to 'Edit', edit_listing_path(@listing) %> |
<% end %>
<%= link_to 'Back', listings_path %>
